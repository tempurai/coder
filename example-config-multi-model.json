{
  "// 模型配置示例 - 支持多种格式": "",
  
  "// 方式1: 字符串格式（向后兼容）": "",
  "model": "gpt-4o-mini",
  
  "// 方式2: 对象格式（推荐用于新配置）": "",
  "// \"model\": {": "",
  "//   \"provider\": \"openai\",": "",
  "//   \"name\": \"gpt-4o-mini\",": "",
